close all; clear all; clc; 
load('train_data.mat');

train.X = train_data.inputs;

[train.mu, train.sigma2] = estimateGaussian(train.X);                  
%  Estimate mu and sigma2


train.p = multivariateGaussian(train.X, train.mu, train.sigma2);     
%  Returns the density of the multivariate normal 
%  at each data point (row) of X

cv.X = cv_data.inputs
cv.p = multivariateGaussian(cv.X, cv.mu, cv.sigma2);                 

[cv.epsilon cv.F1] = selectThreshold(cv.y, pval);
fprintf('Best epsilon found using cross-validation: %e\n', epsilon);
fprintf('Best F1 on Cross Validation Set:  %f\n', F1);
fprintf('   (you should see a value epsilon of about 8.99e-05)\n\n');

outliers = find(p < epsilon);                %  Find the outliers in the training set and plot the
